{
  "properties": [
    {
      "name": "lca.config",
      "type": "java.lang.String",
      "description": "Path to LCA as CODE config directory.",
      "defaultValue": "trusted_library"
    },
    {
      "name": "lca.manifest",
      "type": "java.lang.String",
      "description": "Name of the manifest in the config directory.",
      "defaultValue": "lcaac.yaml"
    },
    {
      "name": "cors.allowed-origin",
      "type": "java.lang.String",
      "description": "CORS allowed origin.",
      "defaultValue": null
    },
    {
      "name": "cors.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable CORS.",
      "defaultValue": false
    },
    {
      "name": "compute.job-size",
      "type": "java.lang.Integer",
      "description": "Job size.",
      "defaultValue": 100
    },
    {
      "name": "resilio-db.url",
      "type": "java.lang.String",
      "description": "Resilio DB URL.",
      "defaultValue": "https://db.resilio.tech"
    },
    {
      "name": "resilio-db.access-token",
      "type": "java.lang.String",
      "description": "Resilio DB access token.",
      "defaultValue": null
    }
  ]
}
